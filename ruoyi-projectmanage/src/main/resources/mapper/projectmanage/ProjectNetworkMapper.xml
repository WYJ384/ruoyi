    <sql id="selectProjectNetworkVo">
        select id, online_name, online_ip, online_time, factory_no, loc, remark3, remark4, remark5, remark6, remark7, remark8, remark9, remark10, remark2, create_by, create_date, update_by, update_date, remark, del_flag, remark1 from project_network
    </sql>

    <select id="selectProjectNetworkList" parameterType="ProjectNetwork" resultMap="ProjectNetworkResult">
        <include refid="selectProjectNetworkVo"/>
        <where>  
            <if test="onlineName != null  and onlineName != ''"> and online_name like concat('%', #{onlineName}, '%')</if>
            <if test="onlineIp != null  and onlineIp != ''"> and online_ip = #{onlineIp}</if>
            <if test="onlineTime != null "> and online_time = #{onlineTime}</if>
            <if test="factoryNo != null  and factoryNo != ''"> and factory_no = #{factoryNo}</if>
            <if test="loc != null  and loc != ''"> and loc = #{loc}</if>
            <if test="remark3 != null  and remark3 != ''"> and remark3 = #{remark3}</if>
            <if test="remark4 != null  and remark4 != ''"> and remark4 = #{remark4}</if>
            <if test="remark5 != null  and remark5 != ''"> and remark5 = #{remark5}</if>
            <if test="remark6 != null  and remark6 != ''"> and remark6 = #{remark6}</if>
            <if test="remark7 != null  and remark7 != ''"> and remark7 = #{remark7}</if>
            <if test="remark8 != null  and remark8 != ''"> and remark8 = #{remark8}</if>
            <if test="remark9 != null  and remark9 != ''"> and remark9 = #{remark9}</if>
            <if test="remark10 != null  and remark10 != ''"> and remark10 = #{remark10}</if>
            <if test="remark2 != null  and remark2 != ''"> and remark2 = #{remark2}</if>
            <if test="createDate != null "> and create_date = #{createDate}</if>
            <if test="updateDate != null "> and update_date = #{updateDate}</if>
            <if test="remark1 != null  and remark1 != ''"> and remark1 = #{remark1}</if>
        </where>
    </select>
    
    <select id="selectProjectNetworkById" parameterType="String" resultMap="ProjectNetworkResult">
        <include refid="selectProjectNetworkVo"/>
        where id = #{id}
    </select>
        
